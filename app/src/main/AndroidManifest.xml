<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kysfw.www.sfw">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.LoginActivity"></activity>
        <activity android:name=".activity.SearchHouseActivity">

        </activity>
        <activity android:name=".activity.SearchResultActivity"/>
        <activity android:name=".activity.HouseDetailActivity"/>
        <activity android:name=".activity.CommonSearchActivity"
            android:windowSoftInputMode="adjustPan|stateHidden"/>
        <activity android:name=".activity.CommonDetailResultActivity"/>
        <activity android:name=".activity.CommonDetailTwoActivity"/>
        <activity android:name=".activity.BigPhotoActivity" />
    </application>

</manifest>